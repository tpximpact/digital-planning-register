{
  "name": "council-public-index",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --quiet",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky",
    "build-start": "npm run build && npm run start",
    "pa11y-wait-on-start": "wait-on http://localhost:3000 && npm run pa11y",
    "run-pa11y": "npm run build && npm run start & npm run pa11y-wait-on-start",
    "pa11y": "pa11y-ci --config .pa11yci"
  },
  "dependencies": {
    "@react-hook/window-size": "^3.1.1",
    "date-fns": "^3.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "govuk-frontend": "^5.2.0",
    "next": "14.1.3",
    "prettier": "^3.2.5",
    "react": "^18",
    "react-dom": "^18",
    "react-paginate": "8.2.0",
    "sass": "^1.71.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "babel-jest": "^29.7.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.3.5",
    "pa11y": "^8.0.0",
    "pa11y-ci": "^3.1.0",
    "typescript": "^5",
    "wait-on": "^7.2.0"
  }
